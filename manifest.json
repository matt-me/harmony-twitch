{

  "manifest_version": 2,
  "name": "TwitchFox",
  "version": "1.0",

  "description": "Replaces chaos with harmony.",
	
  "icons": {
	"48": "icon.png"
  },
	
  "content_scripts": [
	{
	  "matches": ["*://*.twitch.tv/*"],
	  "js": ["mine.js"]
	}
  ],
  "browser_action": {
	"browser_style": true,
	"default_icon": {
		"24": "icon.png"
	},
	"default_title": "Twitch Analyzer",
	"default_popup": "view.html"
  }
}