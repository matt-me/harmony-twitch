{
  "manifest_version": 2,
  "name": "TwitchFox",
  "version": "1.0",
	"permissions": [
		"activeTab",
		"tabs"
  ],
  "description": "Replaces chaos with harmony.",
	
  "icons": {
	"48": "icon.png"
  },
	"background": {
    "scripts": [
			"background.js"
    ]
},
  "browser_action": {
	"browser_style": true,
	"default_icon": {
		"24": "icon.png"
	},
	"default_title": "Twitch Analyzer",
	"default_popup": "view.html"
	}
}